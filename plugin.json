{
	"main": "index.html",
	"logo": "logo.jpg",
    "preload": "preload.js",
	"features": [
		{
		  "code": "jsdoc",
		  "explain": "生成JSDOC",
			"cmds":[{
                "type": "regex",
                "label": "生成箭头函数JSDOC",
                "match": "/async\\s*\\(\\s*\\{[^{}]*\\}\\s*\\)\\s*=>\\s*\\{(?:[^{}]+|\\{(?:[^{}]+|\\{(?:[^{}]+|\\{[^{}]*\\})*\\})*\\})*\\}|[a-zA-Z0-9_$]*\\s*=\\s*\\([^)]*\\)\\s*=>\\s*\\{(?:[^{}]+|\\{(?:[^{}]+|\\{(?:[^{}]+|\\{[^{}]*\\})*\\})*\\})*\\}/m"
            },{
                "type": "regex",
                "label": "生成function函数JSDOC",
                "match": "/(async\\s+)?function\\s*\\w*\\s*\\([^)]*\\)\\s*\\{(?:[^{}]+|\\{(?:[^{}]+|\\{(?:[^{}]+|\\{[^{}]*\\})*\\})*\\})*\\}/m"
            }]

		}
	]
}